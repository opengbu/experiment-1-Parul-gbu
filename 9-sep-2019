//To find area of rectangle
//By Parul Mittal
//19/BHP/017
#include<iostream>
using namespace std;
 int main()
{

float l,b,a;

cout<<"Enter the length of rectangle in meters ";
cin>>l;

cout<<"Enter the breadth of the rectangle in meters ";
cin>>b;

a=l*b;

cout<<"The area of the rectangle is"<<a<<"m^2";

return 1;
}
